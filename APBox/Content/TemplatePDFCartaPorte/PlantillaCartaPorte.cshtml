                                <td>@ubicacion.NombreRemitenteDestinatario</td>
                                <td>@ubicacion.FechaHoraSalidaLlegada</td>

                                @if (ubicacion.NumRegIdTrib != null)
                                {
                                    <td>@ubicacion.NumRegIdTrib</td>
                                }

