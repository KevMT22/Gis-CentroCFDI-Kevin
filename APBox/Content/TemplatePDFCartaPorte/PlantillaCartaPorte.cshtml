                                    <td>@ubicacion.NavegacionTrafico</td>

                                }


                                @* <td>@ubicacion.TipoEstacion</td> *@


                            </tr>
                        </tbody>


                    </table>




                    if (@ubicacion.Domicilio != null)
                    {


                        <b class="encabezado" colspan="5">Domicilio</b>

                        <table table claNumRegIdTribss="table-carta-porte" style="width:100%;">







                            <thead>


                                <tr>
                                    <th>Calle</th>
                                    <th>Numero Exterior</th>
                                    <th>Numero Interior</th>
                                    <th>Colonia</th>
                                    <th>Localidad</th>

                                </tr>
                            </thead>
                            <tbody>

                                <tr>
                                    <th>@ubicacion.Domicilio.Calle</th>
                                    <td>@ubicacion.Domicilio.NumeroExterior</td>
                                    <td>@ubicacion.Domicilio.NumeroInterior</td>
                                    <td>@ubicacion.Domicilio.Colonia</td>
                                    <td>@ubicacion.Domicilio.Localidad</td>


                                </tr>
                            </tbody>


                            <thead>
                                <tr>
                                    <th>Referencia</th>
                                    <th>Municipio</th>
                                    <th>Estado</th>
                                    <th>Pais</th>
                                    <th>Codigo Postal</th>



                                </tr>
                            </thead>




                            <tbody>

                                <tr>
                                    <td>@ubicacion.Domicilio.Referencia</td>
                                    <td>@ubicacion.Domicilio.Municipio</td>
                                    <td>@ubicacion.Domicilio.Estado</td>
                                    <td>@ubicacion.Domicilio.Pais</td>
                                    <td>@ubicacion.Domicilio.CodigoPostal</td>

                                </tr>
                            </tbody>



                        </table>















                    }














                }










                if (@ubicacion.TipoUbicacion == "Destino")
                {
                    <table claNumRegIdTribss="table-carta-porte" style="width:100%;">
                        <caption class="seccion">Ubicaciones</caption>



                        <thead>
                            <tr>
                                <th>Tipo Ubicación</th>
                                <th>ID Ubicacion</th>
                                <th>RFC Remitente Destinatario</th>
                                <th>Nombre Remitente Destinatario</th>
                                <th>Fecha Hora Salida Llegada </th>
                                @if (ubicacion.NumRegIdTrib != null)
                                {
                                    <th>NumRegIdTrib</th>
                                }

                                @if (ubicacion.ResidenciaFiscal != null)
                                {
                                    <th>Residencia Fiscal</th>
                                }
                            </tr>
                        </thead>
                        <tbody>

                            <tr>
                                <th valign="top">@ubicacion.TipoUbicacion</th>

                                <td>@ubicacion.IDUbicacion</td>
                                <td>@ubicacion.RFCRemitenteDestinatario</td>
                                <td>@ubicacion.NombreRemitenteDestinatario</td>
                                <td>@ubicacion.FechaHoraSalidaLlegada</td>

                                @if (ubicacion.NumRegIdTrib != null)
                                {
                                    <td>@ubicacion.NumRegIdTrib</td>
                                }

                                @if (ubicacion.ResidenciaFiscal != null)
                                {
                                    <td>@ubicacion.ResidenciaFiscal</td>
                                }
                            </tr>
                        </tbody>


                        <thead>
                            <tr>

                                @if (ubicacion.NumEstacion != null)
                                {
                                    <th>Num Estación</th>

                                }

                                @if (ubicacion.NombreEstacion != null)
                                {
                                    <th>Nombre Estación</th>

                                }
                                @if (ubicacion.NavegacionTrafico != null)
                                {
                                    <th>Navegacion Trafico</th>

                                }
                                @if (ubicacion.TipoEstacion != null)
                                {
                                    <th>Tipo Estacion</th>
                                }
                                    <th>Distancia Recorrida</th>
                                

                                </tr>
                        </thead>



                        <tbody>

                            <tr>
                                @if (ubicacion.NumEstacion != null)
                                {
                                    <th>@ubicacion.NumEstacion</th>
                                }

                                @if (ubicacion.NombreEstacion != null)
                                {
                                    <td>@ubicacion.NombreEstacion</td>
                                }

                                @if (ubicacion.NavegacionTrafico != null)
                                {
                                    <td>@ubicacion.NavegacionTrafico</td>

                                }

                                @if (ubicacion.TipoEstacion != null)
                                {
                                    <td>@ubicacion.TipoEstacion</td>
                                }
                                    <td>@ubicacion.DistanciaRecorrida</td>

                                </tr>
                        </tbody>


                    </table>



                    if (@ubicacion.Domicilio != null)
                    {


                        <b class="encabezado" colspan="5">Domicilio</b>

                        <table table claNumRegIdTribss="table-carta-porte" style="width:100%;">







                            <thead>


                                <tr>
                                    <th>Calle</th>
                                    <th>Numero Exterior</th>
                                    <th>Numero Interior</th>
                                    <th>Colonia</th>
                                    <th>Localidad</th>

                                </tr>
                            </thead>
                            <tbody>

                                <tr>
                                    <th>@ubicacion.Domicilio.Calle</th>
                                    <td>@ubicacion.Domicilio.NumeroExterior</td>
                                    <td>@ubicacion.Domicilio.NumeroInterior</td>
                                    <td>@ubicacion.Domicilio.Colonia</td>
                                    <td>@ubicacion.Domicilio.Localidad</td>


                                </tr>
                            </tbody>


                            <thead>
                                <tr>
                                    <th>Referencia</th>
                                    <th>Municipio</th>
                                    <th>Estado</th>
                                    <th>Pais</th>
                                    <th>Codigo Postal</th>



                                </tr>
                            </thead>




                            <tbody>

                                <tr>
                                    <td>@ubicacion.Domicilio.Referencia</td>
                                    <td>@ubicacion.Domicilio.Municipio</td>
                                    <td>@ubicacion.Domicilio.Estado</td>
                                    <td>@ubicacion.Domicilio.Pais</td>
                                    <td>@ubicacion.Domicilio.CodigoPostal</td>

                                </tr>
                            </tbody>



                        </table>















                    }














                }

                <div class="salto"></div>

            }





        }
        <!-- Mercancias -->
        @if (Model.CartaPorte != null)
        {

            if (Model.CartaPorte.Mercancias != null)
            {
                foreach (var mercancia in Model.CartaPorte.Mercancias.Mercancia)
                {
                    <table class="table-carta-porte" style="width: 100%;">
                        <caption class="seccion">Mercancías</caption>




                        <thead>
                            <tr>
                                <th>Bienes Transp</th>

                                @if (mercancia.ClaveSTCC != null)
                                {
                                    <th>Clave STCC</th>
                                }

                                <th>Descripción</th>
                                <th>Cantidad</th>
                                <th>Clave Unidad</th>
                                <th>Unidad</th>
                                @if (mercancia.Dimensiones != null)
                                {
                                    <th>Dimensiones</th>
                                }


                                <th>Material Peligroso</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>@mercancia.BienesTransp</td>
                                @if (mercancia.ClaveSTCC != null)
                                {
                                    <td>@mercancia.ClaveSTCC</td>
                                }
                                <td>@mercancia.Descripcion</td>
                                <td>@mercancia.Cantidad</td>
                                <td>@mercancia.ClaveUnidad</td>
                                <td>@mercancia.Unidad</td>
                                @if (mercancia.Dimensiones != null)
                                {
                                    <td>@mercancia.Dimensiones</td>
                                }

                                <td>@mercancia.MaterialPeligroso</td>
                            </tr>
                        </tbody>






                        <thead>
                            <tr>

                                @if (@mercancia.CveMaterialPeligroso != null)
                                {

                                    <th>Cve Material Peligroso</th>
                                }

                                @if (@mercancia.Embalaje != null)
                                {
                                    <th>Embalaje</th>
                                }

                                @if (@mercancia.Embalaje != null)
                                {
                                    <th>Descrip Embalaje</th>
}
