                                {
                                    <th>UUID Comercio Ext</th>
                                }
                            </tr>
                        </thead>


                        <tbody>
                            <tr>
                                @if (@mercancia.CveMaterialPeligroso != null)
                                {
                                    <td>@mercancia.CveMaterialPeligroso</td>
                                }

                                @if (@mercancia.Embalaje != null)
                                {
                                    <td>@mercancia.Embalaje</td>
                                }
                                @if (@mercancia.Embalaje != null)
                                {
                                    <td>@mercancia.DescripEmbalaje</td>
}
                                <td>@mercancia.PesoEnKg</td>
                                <td>@mercancia.ValorMercancia</td>
                                <td>@mercancia.Moneda</td>

                                @if (@mercancia.FraccionArancelaria != null)
                                {
                                    <td>@mercancia.FraccionArancelaria</td>
                                }

                                @if (mercancia.UUIDComercioExt != null)
                                {
                                    <td>@mercancia.UUIDComercioExt</td>
                                }
                            </tr>
                        </tbody>



                    </table>

                    if (mercancia.Pedimentos != null)
                    {
                        foreach (var pedimento in mercancia.Pedimentos)
                        {


                            <b class="encabezado" colspan="5">Pedimentos</b>


                            <table>
                                <thead>
                                    <tr>
                                        <th>Pedimento</th>

                                    </tr>
                                </thead>


                                <tbody>
                                    <tr>
                                        <td>@pedimento.Pedimento</td>

                                    </tr>
                                </tbody>



                            </table>


                        }



                    }









                    if (mercancia.GuiasIdentificacion != null)
                    {
                        foreach (var guiaiden in mercancia.GuiasIdentificacion)
                        {


                            <b class="encabezado" colspan="5">Pedimentos</b>


                            <table>
                                <thead>
                                    <tr>
                                        <th>Numero Guia Identificacion</th>
                                        <th>Descrip Guia Identificacion</th>
                                        <th>Peso Guia Idenntificacion</th>
                                    </tr>
                                </thead>


                                <tbody>
                                    <tr>
                                        <td>@guiaiden.NumeroGuiaIdentificacion</td>
                                        <td>@guiaiden.DescripGuiaIdentificacion</td>
                                        <td>@guiaiden.PesoGuiaIdentificacion</td>

                                    </tr>
                                </tbody>



                            </table>










                        }
                    }




                    if (mercancia.CantidadTransporta != null)
                    {
                        foreach (var cant in mercancia.CantidadTransporta)
                        {



                            <b class="encabezado" colspan="5">Cantidad Transportada</b>


                            <table>
                                <thead>
                                    <tr>
                                        <th>Cantidad</th>
                                        <th>ID Origen</th>
                                        <th>ID Destino</th>
                                        <th>Cves Transporte</th>
                                    </tr>
                                </thead>


                                <tbody>
                                    <tr>
                                        <td>@cant.Cantidad</td>
                                        <td>@cant.IDOrigen</td>
                                        <td>@cant.IDDestino</td>
                                        <td>@cant.CvesTransporte</td>
                                    </tr>
                                </tbody>



                            </table>



                        }







                        <div class="salto"></div>

                    }








